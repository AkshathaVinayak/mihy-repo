def build(path, ci_image) {
    stage("Build") {
        docker.image("egovio/ci-react:v8.4.0-lerna").inside {
            sh "pwd && ls -al web/egov-tradelicence/custom-service/ && cd custom-service/ && rm -rf node_modules && npm set registry http://egov-micro-dev.egovernments.org/verdaccio &&  lerna bootstrap && cd packages/egov-tradelicence && npm run build && cp -r build ../../../egov-tradelicence/";
        }
    }
}

return this;
